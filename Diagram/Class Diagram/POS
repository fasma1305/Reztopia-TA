classDiagram
    class Authentication {
        - nama: String
        - email: String
        - password: String
        + Register()
        + Login()
    }
    
    class Order {
        - tenant: String
        - menu: String
        + Tenant()
        + Menu()
    }
    
    class Checkout {
        - orderItems: List<String>
        + KeranjangPesanan()
        + Pembayaran()
        + DetailPesanan()
    }
    
    class History {
        - transactions: List<String>
        + RiwayatTransaksi()
    }
    
    class Profile {
        - name: String
        - email: String
        - phone: String
        + Profil()
        + Logout()
    }
    
    class OrderManager {
        - pendingOrders: List<String>
        + Pesanan()
    }
    
    class OrderHistory {
        - orderHistory: List<String>
        + RiwayatPesanan()
    }
    
    class Mahasiswa {
        Login ()
        Order ()
        Checkout ()
        History ()
        Profile ()
    }
    
    class Tenant {
        OrderManager ()
        OrderHistory ()
    }
    
    Mahasiswa  -->  Authentication
    Mahasiswa  -->  Order
    Mahasiswa  -->  Checkout
    Mahasiswa  -->  History
    Mahasiswa  -->  Profile
    Tenant  -->  OrderManager
    Tenant  -->  OrderHistory
